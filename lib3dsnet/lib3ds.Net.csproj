<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>disable</Nullable>
        <RootNamespace>$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <AssemblyVersion>1.0.0.0</AssemblyVersion>
        <FileVersion>1.0.0.0</FileVersion>
        <Description>lib3ds-Net is an overall software library for managing 3D-Studio Release 3 and 4 '.3DS' files.</Description>
        <Copyright>Portions Copyright © 2011 by Shinta &lt;shintadono@googlemail.com&gt;\r\nCopyright © 1996-2008 by Jan Eric Kyprianidis &lt;www.kyprianidis.com&gt;</Copyright>
        <PackageProjectUrl>https://github.com/Jeongyong-park/lib3ds.net</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Jeongyong-park/lib3ds.net</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>3ds</PackageTags>
        <TargetFramework>netstandard2.0</TargetFramework>
        <LangVersion>latestmajor</LangVersion>
    </PropertyGroup>

</Project>
